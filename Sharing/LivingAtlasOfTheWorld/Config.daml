  <modules>
    <insertModule id="LivingAtlasOfTheWorld_Module" className="Module1" autoLoad="false" caption="Module1">
      <!-- uncomment to have the control hosted on a separate tab-->
      <tabs>
        <!--<tab id="LivingAtlasOfTheWorld_Tab1" caption="New Tab">
          <group refID="LivingAtlasOfTheWorld_Group1"/>
        </tab>-->
      </tabs>
      <groups>
        <!-- comment this out if you have no controls on the Addin tab to avoid
              an empty group-->
        <group id="LivingAtlasOfTheWorld_Group1" caption="Group 1" appearsOnAddInTab="true" keytip="z1">
          <!-- host controls within groups -->
          <splitButton refID="LivingAtlasOfTheWorld_AddLayerButton" />
        </group>
      </groups>
      <controls>
        <!-- add your controls here -->
        <button id="LivingAtlasOfTheWorld_AddLayerButton_Items_Button1" caption="Living Atlas of the World" className="AddLayerButton_button1" loadOnClick="true" smallImage="Images\EvilGenius16.png" largeImage="Images\EvilGenius32.png" keytip="z2">
          <tooltip heading="Browse Esri Map Layers">Show the 'Browse Esri Map Layers' dialog with data from ArcGIS Online<disabledText /></tooltip>
        </button>
        <button id="LivingAtlasOfTheWorld_AddLayerButton_Items_Button2" caption="AddLayerButton 2" className="AddLayerButton_button2" loadOnClick="true" smallImage="Images\EvilGenius16.png" largeImage="Images\EvilGenius32.png" keytip="z3">
          <tooltip heading="Split Button 2">ToolTip<disabledText /></tooltip>
        </button>
        <button id="LivingAtlasOfTheWorld_AddLayerButton_Items_Button3" caption="AddLayerButton 3" className="AddLayerButton_button3" loadOnClick="true" smallImage="Images\EvilGenius16.png" largeImage="Images\EvilGenius32.png" keytip="z4">
          <tooltip heading="Split Button 3">ToolTip<disabledText /></tooltip>
        </button>
      </controls>
      <splitButtons>
        <splitButton id="LivingAtlasOfTheWorld_AddLayerButton">
          <button refID="LivingAtlasOfTheWorld_AddLayerButton_Items_Button1" />
          <!--<button refID="LivingAtlasOfTheWorld_AddLayerButton_Items_Button2" />
          <button refID="LivingAtlasOfTheWorld_AddLayerButton_Items_Button3" />-->
        </splitButton>
      </splitButtons>
    </insertModule>
  </modules>
